      },
      {
        "command": "adonis-vscode-extension.docs.adonis",
        "title": "üìú AdonisJS(Docs) Open Docs"
      },
      {
        "command": "adonis-vscode-extension.docs.japa",
        "title": "üìú AdonisJS(Docs) Open Japa Docs"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/adonis/routes.json"
      },
      {
        "language": "edge",
        "path": "./snippets/edge/tags.jsonc"
      }
    ],
    "languages": [
      {
        "id": "edge",
        "aliases": [
          "Edge Template",
          "Edge",
          "edge"
        ],
        "extensions": [
          ".edge",
          ".edge.html"
        ],
        "configuration": "./syntaxes/edge.configuration.jsonc"
      }
    ],
    "grammars": [
      {
        "language": "edge",
        "scopeName": "text.html.edge",
        "path": "./syntaxes/edge.tmLanguage.json",
        "embeddedLanguages": {
          "source.css": "css",
          "source.js": "javascript",
          "meta.embedded.block.javascript": "javascript"
        }
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": ".adonisrc.json",
        "url": "./schemas/adonisrc.jsonc"
      }
    ]
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "watch": "pnpm dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "version": "npm run build",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "test:electron": "tsc -p ./ && node ./out/test/run_electron_tests.js",
    "test:pure": "ts-node test/run_pure_tests.ts",
    "test": "npm run test:electron && npm run test:pure",
    "quick:test": "ts-node test/run_pure_tests.ts",
    "coverage": "c8 npm run quick:test",
    "build:webpack": "webpack --mode production --devtool hidden-source-map",
    "vscode:prepublish": "pnpm build:webpack",
    "vscode:package": "vsce package",
    "vscode:publish": "bumpp --commit --push --tag && vsce publish",
    "vscode:publish-prerelease": "bumpp --commit --push --tag && vsce publish --pre-release",
    "publish": "ts-node scripts/publish.ts"
  },
  "dependencies": {
    "@julr/module-methods-extractor": "^0.0.1",
    "common-path-prefix": "^3.0.0",
    "dedent": "^0.7.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.12",
    "lodash.groupby": "^4.6.0",
    "slash": "^3.0.0",
    "vscode-ext-help-and-feedback-view": "^1.0.1",
    "vscode-html-languageservice": "^5.0.6",
    "vscode-languageserver-types": "^3.17.3"
  },
  "devDependencies": {
    "@japa/assert": "^1.4.1",
    "@japa/file-system": "^1.0.1",
    "@japa/runner": "^2.5.1",
    "@japa/snapshot": "1.0.1-3",
    "@japa/spec-reporter": "^1.3.3",
    "@julr/eslint-config": "^0.3.2",
    "@types/dedent": "^0.7.0",
    "@types/lodash.groupby": "^4.6.7",
    "@types/node": "14.x",
    "@types/semver": "^7.5.0",
    "@types/sinon": "^10.0.15",
    "@types/vscode": "^1.65.0",
    "@vscode/test-electron": "^2.3.2",
    "bumpp": "^9.1.1",
    "c8": "^7.14.0",
    "change-case": "^4.1.2",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "ovsx": "^0.8.1",
    "prettier": "^2.8.8",
    "semver": "^7.5.4",
    "sinon": "^15.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
  "c8": {
    "exclude": [
      "test",
      "out"
    ],
    "reporter": [
      "text",
      "html"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "openapi-types",
        "@babel/core"
      ]
    }
  },
  "eslintConfig": {
    "ignorePatterns": [
      "out",
      "syntaxes",
      "test/fixtures",
      "snippets",
      "schemas"
    ],
    "extends": [
      "@julr/eslint-config-ts",
      "@julr/eslint-config-prettier"
    ],
    "rules": {
      "no-useless-escape": "off",
      "sonarjs/no-duplicate-string": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  }
}
